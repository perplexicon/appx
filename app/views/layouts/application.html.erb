<!DOCTYPE html>
<html>

  <head>
    <title>Appx</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <header>
      <% if signed_in? %>
        <%= current_user.email %>
        <%= link_to "Sign out", session_path, method: :delete %>
      <% end %>
    </header>

    <%= yield %>

  </body>
</html>
